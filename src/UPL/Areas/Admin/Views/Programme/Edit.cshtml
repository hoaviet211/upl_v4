@model UPL.Domain.Entities.Programme
@{
    ViewData["Title"] = "Edit Programme";
}
<h2>Edit Programme</h2>
<form asp-action="Edit" method="post" class="mt-3">
    <input type="hidden" asp-for="Id" />
    <div class="mb-3">
        <label asp-for="ProgrammeName" class="form-label"></label>
        <input asp-for="ProgrammeName" class="form-control" />
        <span asp-validation-for="ProgrammeName" class="text-danger"></span>
    </div>
    <div class="mb-3">
        <label asp-for="AdmissionCycle" class="form-label"></label>
        <input asp-for="AdmissionCycle" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-action="Index" class="btn btn-secondary">Back</a>
    </form>
@section Scripts{
    <partial name="_ValidationScriptsPartial" />
}

